<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.easycode.dao.UserTableDao">

    <resultMap type="com.springboot.easycode.entity.UserTable" id="UserTableMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="headName" column="head_name" jdbcType="VARCHAR"/>
        <result property="headSix" column="head_six" jdbcType="VARCHAR"/>
        <result property="isOnjob" column="is_onjob" jdbcType="INTEGER"/>
        <result property="isOnline" column="is_online" jdbcType="INTEGER"/>
        <result property="note" column="note" jdbcType="VARCHAR"/>
        <result property="passWord" column="pass_word" jdbcType="VARCHAR"/>
        <result property="phone" column="phone" jdbcType="VARCHAR"/>
        <result property="userId" column="user_id" jdbcType="VARCHAR"/>
        <result property="userName" column="user_name" jdbcType="VARCHAR"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="UserTableMap">
        select
          id, create_time, head_name, head_six, is_onjob, is_online, note, pass_word, phone, user_id, user_name
        from test.user_table
        where id = #{id}
    </select>

    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="UserTableMap">
        select
          id, create_time, head_name, head_six, is_onjob, is_online, note, pass_word, phone, user_id, user_name
        from test.user_table
        limit #{offset}, #{limit}
    </select>

    <!--通过实体作为筛选条件查询-->
    <select id="queryAll" resultMap="UserTableMap">
        select
          id, create_time, head_name, head_six, is_onjob, is_online, note, pass_word, phone, user_id, user_name
        from test.user_table
        <where>
            <if test="id != null">
                and id = #{id}
            </if>
            <if test="createTime != null">
                and create_time = #{createTime}
            </if>
            <if test="headName != null and headName != ''">
                and head_name = #{headName}
            </if>
            <if test="headSix != null and headSix != ''">
                and head_six = #{headSix}
            </if>
            <if test="isOnjob != null">
                and is_onjob = #{isOnjob}
            </if>
            <if test="isOnline != null">
                and is_online = #{isOnline}
            </if>
            <if test="note != null and note != ''">
                and note = #{note}
            </if>
            <if test="passWord != null and passWord != ''">
                and pass_word = #{passWord}
            </if>
            <if test="phone != null and phone != ''">
                and phone = #{phone}
            </if>
            <if test="userId != null and userId != ''">
                and user_id = #{userId}
            </if>
            <if test="userName != null and userName != ''">
                and user_name = #{userName}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true">
        insert into test.user_table(create_time, head_name, head_six, is_onjob, is_online, note, pass_word, phone, user_id, user_name)
        values (#{createTime}, #{headName}, #{headSix}, #{isOnjob}, #{isOnline}, #{note}, #{passWord}, #{phone}, #{userId}, #{userName})
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update test.user_table
        <set>
            <if test="createTime != null">
                create_time = #{createTime},
            </if>
            <if test="headName != null and headName != ''">
                head_name = #{headName},
            </if>
            <if test="headSix != null and headSix != ''">
                head_six = #{headSix},
            </if>
            <if test="isOnjob != null">
                is_onjob = #{isOnjob},
            </if>
            <if test="isOnline != null">
                is_online = #{isOnline},
            </if>
            <if test="note != null and note != ''">
                note = #{note},
            </if>
            <if test="passWord != null and passWord != ''">
                pass_word = #{passWord},
            </if>
            <if test="phone != null and phone != ''">
                phone = #{phone},
            </if>
            <if test="userId != null and userId != ''">
                user_id = #{userId},
            </if>
            <if test="userName != null and userName != ''">
                user_name = #{userName},
            </if>
        </set>
        where id = #{id}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from test.user_table where id = #{id}
    </delete>

</mapper>